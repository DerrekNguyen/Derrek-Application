<Window x:Class="Derrek_Application.UI.DisplayAssignment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Derrek_Application.UI"
        mc:Ignorable="d"
        Title="Display Assignment" Height="450" Width="800"
        Background="#272537">
   <Grid>
      <Rectangle x:Name="createAssignmentBox" Stroke="Black" Margin="0,0,629,335" Fill="White"/>
      <ListBox x:Name="assignmentListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,99,629,0"/>
      <Button x:Name="createAssignmentButton" Content="Button" HorizontalAlignment="Left" Margin="19,31,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.325,0.106" Height="37" Width="51"/>
      <Label x:Name="createAssignmentLabel" Content="Label" HorizontalAlignment="Left" Height="37" Margin="86,31,0,0" VerticalAlignment="Top" Width="68"/>

   </Grid>
</Window>
